    "mocha": "9.1.1",
    "nyc": "15.1.0",
    "sinon": "11.1.2"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "fast-glob": "3.2.12",
    "prompts": "2.4.2",
    "rfdc": "1.3.0"
