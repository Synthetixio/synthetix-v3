{
  "all": true,
  "extension": [
    ".ts"
  ],
  "include": [
    "src/tasks/**/*.ts",
    "src/subtasks/**/*.ts",
    "src/internal/**/*.ts"
  ],
  "exclude": [
    "**/*.d.ts"
  ],
  "check-coverage": true,
  "reporter": [
    "lcov",
    "text"
  ],
  "branches": 1,
  "lines": 1,
  "functions": 1,
  "statements": 1
}
